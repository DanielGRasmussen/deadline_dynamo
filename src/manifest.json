{
    "manifest_version": 3,
    "name": "Deadline Dynamo",
    "description": "A chrome extension that helps the user to organize and plan their Canvas assignments.",
    "version": "0.1",
    "author": "",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "storage",
      "alarms"
    ],
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "background": {
      "service_worker": "background/index.js"
    },
    "content_scripts": [{
      "matches": ["*://byui.instructure.com"],
      "js": ["content_scripts.js/index.js"]
    }]
  }
  